{
    "name": "imangazaliev/didom",
    "description": "Simple and fast HTML parser",
    "type": "library",
    "keywords": ["didom", "parser", "html", "xml"],
    "license": "MIT",
    "homepage": "https://github.com/Imangazaliev/DiDOM",
    "authors": [
        {
            "name": "Imangazaliev Muhammad",
            "email": "imangazalievm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "DiDom\\": "src/DiDom/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "DiDom\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2"
        }
    }
}
